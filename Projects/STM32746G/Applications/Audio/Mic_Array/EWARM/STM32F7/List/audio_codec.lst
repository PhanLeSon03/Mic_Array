    523            Codec_CtrlInterface_DeInit();
    524            
    525            /* Deinitialize the Codec audio interface (I2S) */
    526            Codec_AudioInterface_DeInit(); 
    527            
    528            /* Return communication control value */
    529            return counter;  
    530          }
