    509          static uint32_t Codec_DeInit(void)
    510          {
    511            uint32_t counter = 0; 
    512          
    513            /* Reset the Codec Registers */
    514            Codec_Reset();  
    515            
    516            /* Keep Codec powered OFF */
    517            counter += Codec_WriteRegister(0x02, 0x01);    
